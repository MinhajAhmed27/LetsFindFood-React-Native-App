{
  "expo": {
    "name": "LetsFindFood",
    "slug": "Lets-Find-Food",
    "android": {
      "package": "com.marmin.LetsFindFood",
      "googleServicesFile": "./google-services.json",
      "versionCode": 1
    },
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons-food-lg.png",
    "splash": {
      "image": "./assets/Logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ff5c5c"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    }
  }
}
